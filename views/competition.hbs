<main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
  <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Competition Rounds</h1>
    <div class="btn-toolbar mb-2 mb-md-0">
      <div class="btn-group me-2">
        <button type="button" class="btn btn-sm btn-outline-secondary" onclick="exportUsers()">Export(not working yet)</button>
      </div>
    </div>
  </div>
  <p>{{noTeamString}}</p>
  <div class="row">
    <div class="col-5 table-responsive">
      <table class="table table-striped table-sm">
        <thead>
          <tr>
            <th>Round Name</th>
            <th>Division</th>
            <th>Start Time</th>
            <th>View Details</th>
          </tr>
        </thead>
        <tbody>
          {{#each rounds}}
          <tr>
            <td>{{title}}</td>
            <td>{{division}}</td>
            <td>{{datePublish}}</td>
            <td>
              <a href="#" onclick="filterRounds('{{_id}}', '{{title}}')">
                <p data-feather="arrow-right"></p>
              </a>
            </td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
    <div class="col-7">
      <br>
      <div class="card">
        <div id="memberView" class="card-body">
          View Details of a round by clicking the
          <p data-feather="arrow-right"></p> of one of the teams
        </div>
      </div>
    </div>
  </div>
</main>